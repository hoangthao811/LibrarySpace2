<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.libraryspace.libraryspace" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".cal_group"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name = "com.libraryspace.cal_group"/>
                    <category android:name = "android.intent.category.DEFAULT"/>
            </intent-filter>
        </activity>
        <activity
            android:name=".Indiv_cal"
            android:label="@string/title_activity_indiv_cal" >
        </activity>
        <activity
            android:name=".MediaCal"
            android:label="@string/title_activity_media_cal" >
        </activity>
    </application>

</manifest>
